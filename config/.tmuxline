# This tmux statusbar config was created by tmuxline.vim
# on Wed, 10 Oct 2018

set -g status-justify "left"
set -g status "on"
set -g status-style "none"
set -g message-command-style bg="colour240"
set -g status-left-length "100"
set -g pane-active-border-style fg="colour216"
set -g status-bg "colour238"
set -g message-command-style fg="colour252"
set -g pane-border-style fg="colour240"
set -g message-style bg="colour240"
set -g status-left-style "none"
set -g status-right-style "none"
set -g status-right-length "100"
set -g message-style fg="colour252"
setw -g window-status-style fg="colour248"
setw -g window-status-style "none"
setw -g window-status-activity-style bg="colour238"
setw -g window-status-activity-style "none"
setw -g window-status-activity-style fg="colour216"
setw -g window-status-separator ""
setw -g window-status-style bg="colour238"
set -g status-left "#[fg=colour238,bg=colour216] #S #[fg=colour216,bg=colour238,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=colour244,bg=colour238,nobold,nounderscore,noitalics]#[fg=colour238,bg=colour244] %R "
setw -g window-status-format "#[fg=colour238,bg=colour238,nobold,nounderscore,noitalics]#[default] #I #W #[fg=colour238,bg=colour238,nobold,nounderscore,noitalics]"
setw -g window-status-current-format "#[fg=colour238,bg=colour240,nobold,nounderscore,noitalics]#[fg=colour252,bg=colour240] #I #W #[fg=colour240,bg=colour238,nobold,nounderscore,noitalics]"
